{
  "name": "projex-snippets",
  "displayName": "Projex Snippets",
  "version": "0.1.0",
  "description": "List of snnipets to work with react, typescript and more",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "markdown",
    "snippets",
    "markdown snippets",
    "extended markdown snippets",
    "typescript snippets",
    "typescript react snippets",
    "react snippets",
    "projex snippets",
    "projex"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Maik3345/projex-snippets"
  },
  "license": "MIT",
  "author": {
    "name": "Maik Restrepo"
  },
  "publisher": "MaikRestrepo",
  "main": "./out/extension.js",
  "scripts": {
    "prepare": "npx husky",
    "clean": "node scripts/clean-dist.js",
    "build": "npm run package && vsce package",
    "package": "npm run build:instructions && npm run compile",
    "build:instructions": "node scripts/generate-instructions.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "chatParticipants": [
      {
        "id": "projex",
        "name": "projex",
        "description": "Asistente especializado en Pull Requests, documentaci칩n, QA y desarrollo",
        "isSticky": true
      }
    ],
    "commands": [
      {
        "command": "projex-snippets.clearChat",
        "title": "Clear Projex Chat",
        "category": "Projex"
      },
      {
        "command": "projex-snippets.syncInstructions",
        "title": "Sincronizar Instrucciones Projex",
        "category": "Projex"
      },
      {
        "command": "projex-snippets.checkInstructions",
        "title": "Verificar Estado de Instrucciones Projex",
        "category": "Projex"
      },
      {
        "command": "projex-snippets.openHelp",
        "title": "Abrir Ayuda de Instrucciones Projex",
        "category": "Projex"
      },
      {
        "command": "projex-snippets.activateProjexInstructions",
        "title": "Activar Instrucciones Projex en Workspace",
        "category": "Projex"
      },
      {
        "command": "projex-snippets.toggleAutoSync",
        "title": "Activar/Desactivar Sincronizaci칩n Autom치tica",
        "category": "Projex"
      }
    ],
    "configuration": {
      "title": "Projex Snippets",
      "properties": {
        "projexSnippets.autoSyncInstructions": {
          "type": "boolean",
          "default": false,
          "description": "Sincronizar autom치ticamente las instrucciones de Projex Snippets al iniciar VS Code"
        }
      }
    },
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.code-snippets"
      },
      {
        "language": "markdown",
        "path": "./snippets/markdown.code-snippets"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^18.19.122",
    "@types/vscode": "^1.102.0",
    "husky": "^9.1.7",
    "typescript": "^5.0.0",
    "vsce": "^2.15.0"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "icon": "img/logo.png",
  "galleryBanner": {
    "color": "#89009a",
    "theme": "dark"
  }
}